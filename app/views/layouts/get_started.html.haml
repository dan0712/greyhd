!!!
%html(ng-app="stubsApp")
  %head
    %meta(charset="utf-8")
    %meta(http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1")
    %meta(name="viewport" content="width=device-width, initial-scale=1.0")
    %title greyhd
    = stylesheet_link_tag "application", media: "all"
    = stylesheet_link_tag "https://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700"
    = javascript_include_tag "https://ajax.googleapis.com/ajax/libs/angularjs/1.5.3/angular.js"
    = javascript_include_tag "https://ajax.googleapis.com/ajax/libs/angularjs/1.5.3/angular-animate.js"
    = javascript_include_tag "https://ajax.googleapis.com/ajax/libs/angularjs/1.5.0/angular-sanitize.js"
    = javascript_include_tag "https://angular-ui.github.io/bootstrap/ui-bootstrap-tpls-1.3.3.js"
    = javascript_include_tag "stubs/stubs"
  %body
    = render 'stubs/partials/getting_started/header_getting_started'
    = yield
